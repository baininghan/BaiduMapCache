<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>BaiduMapCache</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>InitServlet</servlet-name>
    <servlet-class>cn.cstor.map.baidumapcache.InitServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>Cache</servlet-name>
    <servlet-class>cn.cstor.map.baidumapcache.CacheServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>MakeCache</servlet-name>
    <servlet-class>cn.cstor.map.baidumapcache.MakeCacheServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Cache</servlet-name>
    <url-pattern>/cache/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>MakeCache</servlet-name>
    <url-pattern>/makecache/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>InitServer</display-name>
    <servlet-name>InitServer</servlet-name>
    <servlet-class>cn.cstor.map.baidumapcache.InitServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>InitServer</servlet-name>
    <url-pattern>/InitServer</url-pattern>
  </servlet-mapping>
</web-app>